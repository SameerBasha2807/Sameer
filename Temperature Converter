def convert(temp,unit):
    if unit ==  f:
        n = (temp-32)*5/9
        print('The Fahrenheit temperature in Celsius is ' ,n,'C')
    elif unit ==   c:
        n = (temp*9/5)+32
        print("The Celcius temperature in Fahrenheit is " ,n,"F")
    else :
        print(Invalid)
temp = float(input("Enter temperature"))
unit = input("Enter either f or c only")
convert(temp,unit)
